{
  "name": "nuxt3-starter",
  "type": "module",
  "description": "A better nuxt3 starter template",
  "private": false,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "lint:ts": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
